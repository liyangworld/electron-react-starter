{
  "name": "electron-react-starter",
  "version": "1.3.0",
  "private": false,
  "main": "main.js",
  "homepage": "./",
  "description": "electron-react-starter first",
  "author": {
    "name": "liyang",
    "email": "1909457483@qq.com"
  },
  "repository": "https://github.com/liyangworld/electron-react-starter.git",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"wait-on http://localhost:3000 && electron .\" \"cross-env BROWSER=none npm start\"",
    "buildMain": "webpack --config webpack.config.main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder -p always",
    "prepack": "npm run build && npm run buildMain",
    "predist": "npm run build && npm run buildMain",
    "prerelease": "npm run build && npm run buildMain"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "electron-updater": "^4.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.4.0",
    "electron-builder": "^22.6.0",
    "electron-is-dev": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "wait-on": "^4.0.2",
    "webpack-cli": "^3.3.11"
  }
}